{
	"name": "midi-relay",
	"productName": "midi-relay",
	"version": "3.4.0",
	"description": "Listens for HTTP requests with JSON payload and relays MIDI/MSC commands on local ports.",
	"license": "MIT",
	"repository": "josephdadams/midi-relay",
	"author": {
		"name": "Joseph Adams",
		"email": "josephdadams@gmail.com",
		"url": "https://josephadams.dev"
	},
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"lint": "xo",
		"test": "npm run lint",
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder --macos --linux --windows",
		"distWin": "electron-builder --windows",
		"release": "np"
	},
	"dependencies": {
		"body-parser": "^1.18.3",
		"electron-context-menu": "^3.0.0",
		"electron-debug": "^3.2.0",
		"electron-positioner": "^4.1.0",
		"electron-store": "^8.0.0",
		"electron-unhandled": "^3.0.2",
		"electron-updater": "^4.3.8",
		"electron-util": "^0.15.1",
		"express": "^4.18.1",
		"http": "^0.0.1-security",
		"jquery": "^3.3.1",
		"jzz": "^1.6.1",
		"lodash": "^4.17.21",
		"mdns-js": "^1.0.3",
		"request": "^2.88.2",
		"socket.io": "^4.5.1"
	},
	"devDependencies": {
		"electron": "^18.3.7",
		"electron-builder": "^24.3.0",
		"np": "^7.5.0",
		"xo": "^0.54.2"
	},
	"xo": {
		"envs": [
			"node",
			"browser"
		]
	},
	"np": {
		"publish": false,
		"releaseDraft": false
	},
	"build": {
		"appId": "com.josephadams.midi-relay",
		"mac": {
			"category": "public.app-category.productivity",
			"darkModeSupport": true
		},
		"dmg": {
			"iconSize": 160,
			"contents": [
				{
					"x": 180,
					"y": 170
				},
				{
					"x": 480,
					"y": 170,
					"type": "link",
					"path": "/Applications"
				}
			]
		}
	}
}
